{
  "name": "enlearn-warehouse",
  "version": "0.6.0",
  "description": "Promise based data storage.",
  "license": "MIT",
  "homepage": "https://github.com/engagedlearning/warehouse",
  "bugs": "https://github.com/engagedlearning/warehouse/issues",
  "repository": {
    "url": "https://github.com:engagedlearning/warehouse",
    "type": "git"
  },
  "main": "dist/bundle.js",
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --write \"**/*.md\" \"**/*.js\" \"**/*.json\"",
    "test": "jest --silent",
    "release": "release-it",
    "build": "rimraf dist && yarn build:js && yarn make-package",
    "build:js": "rollup -c",
    "make-package": "node scripts/make-package.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**.{md,json}": [
      "prettier --write",
      "git add"
    ],
    "**.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@enlearn/js-helpers": "EngagedLearning/js-helpers.git#^0.0.x"
  },
  "devDependencies": {
    "archiver": "^3.0.0",
    "fs-jetpack": "^2.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@enlearn/eslint-config": "^1.4.0",
    "babel-jest": "^24.1.0",
    "changelog-parser": "^2.7.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-promise": "^4.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "moment": "^2.24.0",
    "prettier": "^1.16.4",
    "release-it": "^12.3.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
